<?xml version='1.0' encoding='utf-8'?>
<widget id="com.alefesouza.quasartest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>quasar-electron</name>
    <description>Quasar application sample using Electron and Meetup API.</description>
    <author email="contact@alefesouza.com" href="https://alefesouza.com">
        Alefe Souza
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
        <splash density="land-ldpi" src="res/screen/android/screen-ldpi-landscape.png" />
        <splash density="land-mdpi" src="res/screen/android/screen-mdpi-landscape.png" />
        <splash density="land-hdpi" src="res/screen/android/screen-hdpi-landscape.png" />
        <splash density="land-xhdpi" src="res/screen/android/screen-xhdpi-landscape.png" />
        <splash density="port-ldpi" src="res/screen/android/screen-ldpi-portrait.png" />
        <splash density="port-mdpi" src="res/screen/android/screen-mdpi-portrait.png" />
        <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash density="port-xhdpi" src="res/screen/android/screen-xhdpi-portrait.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="res/icon/ios/icon-57.png" width="57" />
        <icon height="114" src="res/icon/ios/icon-57-2x.png" width="114" />
        <icon height="72" src="res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="res/icon/ios/icon-72-2x.png" width="144" />
        <splash height="768" src="res/screen/ios/screen-ipad-landscape" width="1024" />
        <splash height="1536" src="res/screen/ios/screen-ipad-landscape-2x" width="2048" />
        <splash height="1024" src="res/screen/ios/screen-ipad-portrait" width="768" />
        <splash height="2048" src="res/screen/ios/screen-ipad-portrait-2x" width="1536" />
        <splash height="640" src="res/screen/ios/screen-iphone-landscape-2x" width="960" />
        <splash height="320" src="res/screen/ios/screen-iphone-landscape" width="480" />
        <splash height="960" src="res/screen/ios/screen-iphone-portrait-2x" width="640" />
        <splash height="480" src="res/screen/ios/screen-iphone-portrait" width="320" />
        <splash height="1136" src="res/screen/ios/screen-iphone-portrait-568h-2x" width="640" />
    </platform>
    <engine name="android" spec="^7.0.0" />
</widget>
